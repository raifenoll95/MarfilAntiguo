@using Marfil.Dom.Persistencia.Model
@using Marfil.Dom.Persistencia.Model.FicherosGenerales
@using Marfil.Dom.Persistencia.Model.Interfaces
@using Resources
@model Marfil.Dom.Persistencia.Model.FicherosGenerales.MunicipiosModel

@{
    ViewBag.Title = ((IToolbar)Model).Toolbar.Titulo;
    var objModel = Model as MunicipiosModel;
}

<h2>@General.LblBorrar</h2>

<h3>@General.LblMensajeConfirmacionBorrado</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">

        <dt>
            Pais
        </dt>

        <dd>
            @objModel.DescripcionPais
        </dd>
            <dt>
                Provincia
            </dt>

            <dd>
                @objModel.DescripcionProvincia
            </dd>
                <dt>
                    Municipio
                </dt>

                <dd>
                    @objModel.Nombre
                </dd>
        
       
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <button class="btn btn-info" type="Submit" ng-disabled="!MainFormIsEnable">
                <i class="ace-icon fa fa-trash bigger-110"></i>
                @General.LblContinuar
            </button>

            <a class="btn btn-info" href="@Url.Action("Index")">
                <i class="ace-icon fa fa-undo bigger-110"></i>
                @General.LblCancelar
            </a>
            
        </div>
    }
</div>
