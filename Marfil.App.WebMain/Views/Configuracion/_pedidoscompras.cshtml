
@using Marfil.App.WebMain.Misc
@using Marfil.Dom.Persistencia.Helpers
@using Marfil.Inf.ResourcesGlobalization.Textos.Entidades
@model Marfil.Dom.Persistencia.Model.ConfiguracionModel

<div class="form-group">

    <div class="form-group">
        @Html.LabelFor(f => Model.Estadopedidoscomprasinicial, new {@class = "control-label col-md-2"})
        <div class="col-md-10">
            @Html.DropDownListFor(f => Model.Estadopedidoscomprasinicial, Model.Estadospedidoscompras.Select(f => new SelectListItem() {Value = f.CampoId, Text = f.Descripcion}),new { @class = "form-control" })
        </div>
    </div>


    <div class="form-group">
        @Html.LabelFor(f => Model.Estadopedidoscomprasparcial, new {@class = "control-label col-md-2"})
        <div class="col-md-10">
            @Html.DropDownListFor(f => Model.Estadopedidoscomprasparcial, Model.Estadospedidoscompras.Select(f => new SelectListItem() {Value = f.CampoId, Text = f.Descripcion}), new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(f => Model.Estadopedidoscomprastotal, new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownListFor(f => Model.Estadopedidoscomprastotal, Model.Estadospedidoscompras.Select(f => new SelectListItem() { Value = f.CampoId, Text = f.Descripcion }), new { @class = "form-control" })
        </div>
    </div>
</div>


