@using Marfil.Dom.Persistencia.ServicesView.Servicios
@using Resources
@model IModelView
@{
  
    
  
  
}




@using (Html.BeginForm("CreateOperacion", "Empresas", FormMethod.Post, new { @class = "form-horizontal" ,id = "formEmpresa" }))
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="mantenimiento" value="@ViewBag.Mantenimiento" />
    <div class="form-horizontal">
        


        @Html.Partial("UsersControls/_empresa", Model)

        <div class="form-group">
            <div class="col-md-12 margin-top">
                <button class="btn btn-info" type="Submit" ng-disabled="!MainFormIsEnable">
                    <i class="ace-icon fa fa-check bigger-110"></i>
                    @General.LblCrear
                </button>
            </div>
        </div>
    </div>
}
